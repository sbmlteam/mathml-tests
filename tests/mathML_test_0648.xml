<math xmlns="http://www.w3.org/1998/Math/MathML">
  <semantics>
    <apply>
      <plus/>
      <ci> a </ci>
      <apply>
        <power/>
        <ci> b </ci>
        <ci> c </ci>
      </apply>
    </apply>
    <annotation encoding="infix-input">a + (b ^ c)</annotation>
    <annotation encoding="infix-output">a + b^c</annotation>
  </semantics>
</math>
